{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "declaration": true,
        "lib": ["es6", "dom", "es2015.iterable"]
    },
    "exclude": [
        "node_modules",
        "examples",
        "examples_webpack"
    ]
}